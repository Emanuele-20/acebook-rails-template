<h1>Sign Up</h1>

<%= form_with scope: :user, url: users_path, local: true  do |form| %>

<%= form.label :name %>
<%= form.text_field :name %>

<%= form.label :email %>
<%= form.email_field :email %>

<%= form.label :password %>
<%= form.password_field :password %>

<%= form.submit "Sign up" %>

<% end %>
